{
  "name": "@atsu/choukai",
  "version": "1.0.0",
  "description": "Map and positioning system for game units in the Choukai engine",
  "main": "dist/choukai.js",
  "module": "dist/choukai.mjs",
  "types": "dist/choukai.d.ts",
  "exports": {
    ".": {
      "import": "./dist/choukai.mjs",
      "require": "./dist/choukai.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "game",
    "maps",
    "positioning",
    "coordinates",
    "typescript",
    "gamedev"
  ],
  "author": "Tsukugi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tsukugi/TakaoEngine.git"
  },
  "bugs": {
    "url": "https://github.com/Tsukugi/TakaoEngine/issues"
  },
  "homepage": "https://github.com/Tsukugi/TakaoEngine#readme",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.1",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "tslib": "^2.6.2",
    "@atsu/atago": "file:../Atago"
  }
}